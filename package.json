{
  "name": "sol-monorepo",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awell-health/sol-scheduling.git"
  },
  "author": {
    "name": "awellHealth"
  },
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter ./packages/* run build",
    "dev": "pnpm -r --filter ./packages/* run dev",
    "lint": "pnpm -r --filter ./packages/* run lint",
    "format": "pnpm -r --filter ./packages/* run format",
    "test": "pnpm -r --filter ./packages/* run test",
    "storybook": "pnpm --filter @awell-health/sol-scheduling run storybook",
    "build-storybook": "pnpm --filter @awell-health/sol-scheduling run build-storybook",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "husky": "^9.1.4"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
