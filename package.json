{
  "name": "sol-monorepo",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awell-health/sol-scheduling.git"
  },
  "author": {
    "name": "Awell Health",
    "url": "https://awellhealth.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:app": "turbo run build --filter scheduling-app",
    "dev": "turbo run dev",
    "dev:app": "turbo run dev --filter scheduling-app",
    "lint": "turbo run lint",
    "format": "pnpm -r run format",
    "test": "turbo run test",
    "storybook": "pnpm --filter @awell-health/sol-scheduling run storybook",
    "build-storybook": "pnpm --filter @awell-health/sol-scheduling run build-storybook",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "husky": "^9.1.4",
    "turbo": "^2.5.8"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
